# Finding items modified by Employee ID: 16
GET cvp*/_search
{
  "query": {
    "bool": { 
      "must": [
        { "match": { "description":   "Sleek Paper Hat"}}
      ],
      "filter": [ 
        { "term":  { "modified_by_employee_id": "16" }}
      ]
    }
  }
}

# Aggregate counts of inventory based on Seasons
GET cvp*/_search
{
  "size": 0, 
  "aggs": {
    "season_aggregation": {
      "terms": {
        "field": "ref_seasonal_flag_cd.keyword"
      }
    }
  }
}

