# Search the first 100 categories
GET fintech*/_search
{
  "size": 0,
  "aggs": {
    "category": {
      "terms": {
        "field": "merchant_category.keyword",
        "size": 100
      }
    }
  },"sort": [
    {
      "aggregations.category.buckets.doc_count": {
        "order": "desc"
      }
    }
  ]
}

# Search the first 100 categories
GET fintech*/_search
{
  "size": 0,
  "aggs": {
    "category": {
      "terms": {
        "field": "merchant_category.keyword",
        "size": 100,
        "order": {
          "_count": "desc"
        }
        
      }
    }
  }
}

#Fuzzy Search for Name
GET dda-accounts/_search
{
  "_source": ["name.first","name.last","dda"], 
  "query": {
    "fuzzy": {
      "name.first": {
        "value": "Vincent"
      }
    }
  }
}

